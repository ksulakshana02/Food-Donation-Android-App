<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.ProfileFragment">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:text="Profile"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047" />


    <TextView
        android:id="@+id/profileUserName"
        android:layout_width="97dp"
        android:layout_height="82dp"
        android:fontFamily="@font/inter_semibold"
        android:text="User Name"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18" />

    <RelativeLayout
        android:id="@+id/profileRl"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.868"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.163">

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/profilePhoto"
            android:layout_width="201dp"
            android:layout_height="255dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.009"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:shapeAppearanceOverlay="@style/ImageStyle_Circle_Round"
            tools:src="@drawable/man" />

    </RelativeLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/logout_btn"
        android:layout_width="330dp"
        android:layout_height="60dp"
        android:fontFamily="@font/inter"
        android:text="Log out"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="16sp"
        android:backgroundTint="#e0e5fd"
        app:cornerRadius="10dp"
        app:icon="@drawable/exit"
        app:iconGravity="end"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.858" />


    <com.google.android.material.button.MaterialButton
        android:id="@+id/UserNearBtn"
        android:layout_width="330dp"
        android:layout_height="60dp"
        android:fontFamily="@font/inter"
        android:text="Users nears me"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="16sp"
        android:backgroundTint="#e0e5fd"
        app:cornerRadius="10dp"
        app:icon="@drawable/group"
        app:iconGravity="end"
        app:iconSize="20dp"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.527" />


    <com.google.android.material.button.MaterialButton
        android:id="@+id/listBtn"
        android:layout_width="330dp"
        android:layout_height="60dp"
        android:fontFamily="@font/inter"
        android:text="My Listings"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="16sp"
        android:backgroundTint="#e0e5fd"
        app:cornerRadius="10dp"
        app:icon="@drawable/list"
        app:iconGravity="end"
        app:iconSize="20dp"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.415" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button4"
        android:layout_width="330dp"
        android:layout_height="60dp"
        android:fontFamily="@font/inter"
        android:text="Location"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        android:textSize="16sp"
        android:backgroundTint="#e0e5fd"
        app:cornerRadius="10dp"
        app:icon="@drawable/black_map"
        app:iconGravity="end"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.636" />



</androidx.constraintlayout.widget.ConstraintLayout>